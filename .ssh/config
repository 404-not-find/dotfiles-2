ControlMaster auto
ControlPath ~/.ssh/master-%r@%h:%p

Include config.private

Host *
    ForwardX11 no
    ForwardAgent no

Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/github_rsa

Host gitlab.com
    HostName gitlab.com
    User coderausch
    IdentityFile ~/.ssh/gitlab_rsa

Host vimmer.net
    HostName vimmer.net
    Port 123
    User mhi
    IdentityFile ~/.ssh/vimmer_rsa

Host furor
    HostName furor-ubu.eecsit.tu-berlin.de
    User mhinz

Host sshgate
    HostName sshgate.tu-berlin.de
    User mhinz
